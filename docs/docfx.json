{
  "metadata": [
    {
      "filter": "filterConfig.yml",
      "src": [
        {
          "files": [
            "**/Cheetah.Kafka.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api/Cheetah.Kafka",
      "properties": {
        "TargetFramework": "net8.0"
      },
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "filter": "filterConfig.yml",
      "src": [
        {
          "files": [
            "**/Cheetah.OpenSearch.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api/Cheetah.OpenSearch",
      "properties": {
        "TargetFramework": "net8.0"
      },
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "filter": "filterConfig.yml",
      "src": [
        {
          "files": [
            "**/Cheetah.Auth.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api/Cheetah.Auth",
      "properties": {
        "TargetFramework": "net8.0"
      },
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**/index.md",
          "api/**.yml",
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Cheetah Nuget Packages",
      "_disableContribution": true,
      "_disableToc": false
    },
    "dest": "../_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}