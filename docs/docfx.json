{
  "metadata": [
    {
      "filter": "filterConfig.yml",
      "src": [
        {
          "files": [
            "**/Cheetah.OpenSearch.csproj",
            "**/Cheetah.Kafka.csproj",
            "**/Cheetah.Auth.csproj"
          ],
          "src": "../"
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net6.0"
      },
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": ["README.md"],
        "src": "../",
        "dest": "./"
      },
      {
        "files": ["api/**.yml", "api/index.md"]
      },
      {
        "files": ["articles/**.md", "articles/**/toc.yml", "toc.yml", "*.md"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "globalMetadata": {
      "_appTitle": "Cheetah.Shared (Nuget Package)",
      "_disableContribution": true,
      "_disableToc": false
    },
    "dest": "../_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "modern"],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}
